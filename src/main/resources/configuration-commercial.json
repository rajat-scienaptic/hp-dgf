        "select01": {
          "isUnknown":"n",
          "cols": ["Big Deal ID","Deal Description","Business Model Description","Partner Desc","Partner Ship Calendar Date","Product ID","Claim Quantity","Claim Partner Unit Rebate"]
        }
      },
      "filter": {
        "filter01": {
          "multiWordMap" : { "ptnr_Date": "Partner Ship Calendar Date"},
          "conditionTypes": ["and"],
          "conditions": ["ptnr_Date > 31-Oct-2018"]
        }
      }
    },
    "XS_CLAIMS": {
      "name": "xs_claims",
      "filePath": "/home/avik/Scienaptic/RnD/data/.csv",
      "outFilePath" : "",
      "select": {
        "select01": {
          "isUnknown":"n",
          "cols": ["Deal ID","Deal Description","Business Model Description","Claim Partner Name","Partner Ship Calendar Date","Manufacturing Product Identifier","Claim Quantity","Claim Partner Unit Rebate USD"]
        }
      },
      "join": {
        "join01": {
          "typeOfJoin": ["left","inner"],
          "joinCriteria": {
            "left": ["Big Deal ID","Base SKU"],
            "right": ["C2B Promo Code","SKU"]
          },
          "selectCriteria": {
            "isUnknown": "y",
            "left": [], "right": ["C2B Promo Code","SKU","Start Date","End Date"]
          }
        },
        "join02": {
          "typeOfJoin": ["inner"],
          "joinCriteria": {
            "left": ["Week End Date"],
            "right": ["Week_End_Date"]
          },
          "selectCriteria": {
            "isUnknown": "y",
            "left": [], "right": ["Week_End_Date"]
          }
        },
        "join03": {
          "typeOfJoin": ["left","inner"],
          "joinCriteria": {
            "left": ["Base SKU"],
            "right": ["SKU"]
          },
          "selectCriteria": {
            "isUnknown": "Y",
            "left": [],
            "right": ["Added","L1: Use Case","L2: Key functionality","L3: IDC Category","L0: Format","Need IFS2?","PLC Status","GA date","ES date","NPD Product Category","NPD Model","Sales Product Name"]
          }
        }
      },
      "group" : {
        "group01": {
          "cols" : ["Week End Date", "Partner Ship Calendar Date", "Partner Desc", "Big Deal ID", "Base SKU", "Claim Partner Unit Rebate"],
          "aggregations": {"sum":{"Claim Quantity":"Sum_Claim Quantity"}},
          "selectCriteria": ["Big Deal ID","Deal Description","Business Model Description","Partner Desc","Partner Ship Calendar Date","Product ID","Claim Quantity","Claim Partner Unit Rebate","Base SKU","Temp Date Calc String","Temp Date Calc","Week End Date"]
        },
        "group02": {
          "cols": ["Partner Desc","Big Deal ID","Base SKU","Week End Date"],
          "aggregations": {"sum":{"Sum_Claim Quantity":"Claim Quantity"}},
          "selectCriteria": ["Week End Date","Partner Ship Calendar Date","Partner Desc","Big Deal ID","Base SKU","Claim Partner Unit Rebate","Sum_Claim Quantity"]
        }
      }
    },

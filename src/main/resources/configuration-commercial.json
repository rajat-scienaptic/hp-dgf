        },
        "join03": {
          "typeOfJoin": ["left","inner"],
          "joinCriteria": {
            "left": ["SKU","C2B Promo Code"],
            "right": ["Base SKU","C2B Promo Code"]
          },
          "selectCriteria": {
            "isUnknown": "y",
            "left": [],
            "right": ["C2B Promo Code","Base SKU"]
          }
        },
        "join04": {
          "typeOfJoin": ["left"],
          "joinCriteria": {
            "left": ["Big Deal ID"], "right": ["C2B Promo Code"]
          },
          "selectCriteria": {
            "isUnknown": "y",
            "left": [],
            "right": []
          }
        }
      },
      "filter": {
        "filter01": {
          "multiWordMap": { "out_promo_date": "Outside Promo Date"},
          "conditionTypes": ["and"],
          "conditions": ["out_promo_date = N"]
        },
        "filter02": {
          "multiWordMap": { "out_promo_date": "Outside Promo Date"},
          "conditionTypes": ["and"],
          "conditions": ["out_promo_date != N"]
        },
        "filter03": {
          "multiWordMap": {},
          "conditionTypes": ["and"],
          "conditions": ["Include = Y"]
        }
      },
      "group" : {
        "group01": {
          "cols" : ["C2B Promo Code","SKU","Start Date","End Date"],
          "aggregations": {"sum" : {"dummy":"dummy"}},
          "selectCriteria": ["BU","SKU","Product","Rebate Amount","Promo Name","C2B Promo Code","Start Date","End Date","Season","Season_Ordered"]
        },
        "group02": {
          "cols": ["Base SKU","C2B Promo Code"],
          "aggregations": {"avg": {"Sum_Sum_Claim Quantity": "Avg_Sum_Sum_Claim Quantity"}},
          "selectCriteria": ["Week End Date","Base SKU","C2B Promo Code"]
        },
        "group03": {
          "cols": ["C2B Promo Code","Base SKU"],
          "aggregations": {"max": {"Week End Date": "Max_Week_End_Date"}},
          "selectCriteria": ["Week End Date","Base SKU","Claim Partner Unit Rebate","C2B Promo Code","Sum_Sum_Claim Quantity","Avg_Sum_Sum_Claim Quantity","Include"]
        },
        "group04" :{
          "cols": ["C2B Promo Code"],
          "aggregations": {"sum": {"dummy": ""}},
          "selectCriteria": ["SKU","C2B Promo Code","Promo Name","Season","BU","Product","Rebate Amount","Week_End_Date","Option Type","Source_season","Source_Season_Ordered","Cal_Month","Cal_Year","Fiscal_Qtr","Fiscal_Year"]
        },
        "group05": {
          "cols": ["SKU","Week_End_Date","Option Type"],
          "aggregations": {"max": {"Rebate Amount": "IR"},
                           "first": {"C2B Promo Code":"First_C2B Promo Code"}},
          "selectCriteria": ["SKU","C2B Promo Code","Promo Name","Season","BU","Product","Rebate Amount","Week.End.Date","Option Type","Source_season","Source_Season.Ordered","Cal.Month","Cal.Year","Fiscal.Qtr","Fiscal.Year"]
        },
        "group06": {
          "cols": ["Week End Date","Base SKU","C2B Promo Code"],
          "aggregations": {"sum": {"Sum_Claim Quantity": "Sum_Sum_Claim Quantity"}},
          "selectCriteria": ["Week End Date","Partner Ship Calendar Date","Partner Desc","Big Deal ID","Base SKU","Claim Partner Unit Rebate","Sum_Claim Quantity","C2B Promo Code","Start Date","End Date","Outside Promo Date"]
        },
        "group07": {
          "cols": ["Week End Date","Base SKU","Claim Partner Unit Rebate","C2B Promo Code"],
          "aggregations": {"sum": {"Sum_Claim Quantity": "Sum_Sum_Claim Quantity"}},
          "selectCriteria": ["Week End Date","Partner Ship Calendar Date","Partner Desc","Big Deal ID","Base SKU","Claim Partner Unit Rebate","Sum_Claim Quantity","C2B Promo Code","Start Date","End Date","Outside Promo Date"]
        }
      },
      "sort" : {
        "sort01": {
          "asc": ["SKU","Week_End_Date"],
          "desc": ["Rebate Amount"]
        }
      },
      "rename" : {
        "rename01": {
          "C2B Promo Code": "First_C2B Promo Code"
        }
      }
    },

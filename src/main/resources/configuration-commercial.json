          "selectCriteria": {
            "isUnknown": "n",
            "left": ["Week End Date","Base SKU","Claim Partner Unit Rebate","C2B Promo Code","Sum_Sum_Claim Quantity"],
            "right": ["Avg_Sum_Sum_Claim Quantity"]
